C  IFLUID.H - BLACK OIL MODEL FLUID PROPERTIES

C  CODE HISTORY:

C  JOHN WHEELER     7/15/97    INITIAL VERSION
C  Joe Eaton        9/10/98    checkout for debug

C*********************************************************************
      REAL*4 PWINIT,SWINIT,SGINIT,DINIT,GFSUB,DGFSUB,DFAC,FRSOI,DFRSOI
      LOGICAL SATTOP, SATALL, FINDGOC, USEGFSUB, REFPRESIN
      INTEGER KRINIT, NBSO, NBOD, NRBW, NBG, NRSO, NPSAT, NROD, MODBO
 
      Real*8 OILVIS,WATVIS,GASVIS,ROKCMP,OSTDEN,WSTDEN,GSTDEN
      Real*8 PCGO0($MXRTAB),BOCON

      COMMON /IFLUID/  OILVIS,  WATVIS,  OSTDEN,  WSTDEN,  GSTDEN,
     &        GASVIS,  PCGO0,   BOCON,
     &        ROKCMP,  PWINIT,  SWINIT,  SGINIT,  DINIT,   FRSOI,
     &        DFRSOI,
     &        KRINIT,  NBSO,    NBOD,    NRBW,    NBG,     NRSO,
     &        NPSAT,   MODBO

C*********************************************************************

C  OILVIS = OIL VISCOSITY, cp

C  WATVIS = WATER VISCOSITY, cp

C  GASVIS = GAS VISCOSITY, cp

C  OSTDEN = OIL STOCK TANK DENSITY, lb/stbo
C           NOTE: OSTDEN, WSTDEN, AND GSTDEN ARE THE SAME QUANITIES AS
C                 STDENO, STDENW, AND STDENG IN UNITSEX.H; ONLY THE UNITS
C                 HAVE BEEN CHANGED TO BE CONSISTANT WITH THE BLACK OIL MODEL

C  WSTDEN = WATER STOCK TANK DENSITY, lb/stbw

C  GSTDEN = GAS STOCK TANK DENSITY, lb/mscf

C  ROKCMP = ROCK COMPRESSABILITY, 1/psi

C  PWINIT = INITIAL WATER PRESSURE AT REFERENCE DEPTH, psi

C  SWINIT = INITIAL WATER SATURATION AT REFERENCE DEPTH

C  SGINIT = INITIAL GAS SATURATION AT REFERENCE DEPTH

C  FRSOI  = INITIAL FRACTION OF GAS SATURATION IN THE OIL PHASE AT REFERENCE
C           DEPTH.  USED ONLY IF SGINIT = 0.  MULTIPLIES Rso

C  DFRSOI = INITIAL DERIVATIVE OF FRSOI WRT DEPTH.  USUALLY NEGATIVE

C  DINIT  = INITIAL REFERENCE DEPTH, ft

C  PCGO0(N) = GAS-OIL CAPILLARY PRESSURE OF ROCK TYPE N FOR Sg = 0

C  BOCON  = DIMENSIONLESS CONSTANT FOR BO MODEL 1

C  KRINIT = INITIAL REFERENCE ROCK TYPE

C  NBSO   = TABLE NUMBER FOR SATURATED OIL FORMATION VOLUME FACTOR

C  NBOD   = TABLE NUMBER FOR DEAD OIL FORMATION VOLUME FACTOR

C  NRBW   = TABLE NUMBER FOR RECRIPCAL WATER FORMATION VOLUME FACTOR

C  NBG    = TABLE NUMBER FOR GAS FORMATION VOLUME FACTOR

C  NRSO   = TABLE NUMBER FOR GAS SOLUBILITY IN OIL

C  NPSAT  = TABLE NUMBER FOR SATURATION PRESSURE

C  MODBO  = BO MODEL NUMBER
